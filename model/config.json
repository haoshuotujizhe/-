{
  "model_name": "convnext_base",
  "num_classes": 152,
  "input_size": [512, 512],
  "batch_size": 8,
  "learning_rate": 0.0001,
  "epochs": 100,
  "use_pretrained": true,
  "mean": [0.485, 0.456, 0.406],
  "std": [0.229, 0.224, 0.225],

  "original_train_dir": "../submission/datasets/original_train",
  "train_dir": "../submission/datasets/train",
  "val_dir": "../submission/datasets/val",
  "train_label_csv": "../submission/datasets/train_labels.csv",
  "val_ratio": 0.15,

  "head_epochs": 12,
  "lr_backbone": 0.000015,
  "lr_head": 0.00012,

  "use_mixup": true,
  "mixup_alpha": 0.20,
  "use_cutmix": true,
  "cutmix_alpha": 0.35,

  "use_ema": true,
  "ema_decay": 0.99995,
  "use_tta": true,
  "stochastic_depth": 0.25,
  "label_smoothing": 0.05,

  "allow_tf32": true,
  "channels_last": true,
  "num_workers": 8,
  "persistent_workers": true,
  "prefetch_factor": 4,

  "use_weighted_sampler": true,
  "use_class_weight_loss": true,

  "tta_val_every": 3,
  "warmup_epochs": 5,
  "min_lr": 1e-7,

  "final_finetune": true,
  "final_finetune_epochs": 25,
  "final_finetune_size": [576, 576]
}