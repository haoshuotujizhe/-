{
  "model_name": "efficientnet_b7",      
  "num_classes": 100,
  "input_size": [600, 600],
  "batch_size": 12,
  "learning_rate": 0.00005,
  "epochs": 60,
  "use_pretrained": true,
  "mean": [0.485, 0.456, 0.406],
  "std": [0.229, 0.224, 0.225],

  "train_dir": "../datasets/train",
  "val_dir": "../datasets/val",
  "test_dir":"../datasets/test",
  "train_label_csv": "../datasets/train_labels.csv",
  "vt_weight_list":[0.15,0],


  "head_epochs": 5,                    
  "lr_backbone": 0.00001,              
  "lr_head": 0.00005,                   
  "use_weighted_sampler": true,        
  "use_mixup": true,
  "mixup_alpha": 0.15,
  "use_cutmix": true,
  "cutmix_alpha": 0.8,
  "use_ema": true,
  "ema_decay": 0.9998,
  "use_tta": true                     
}
